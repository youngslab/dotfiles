
# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

# some more cd aliases
alias c='cd ..'
alias cc='cd ../..'
alias ccc='cd ../../..'
alias cccc='cd ../../../..'
alias ccccc='cd ../../../../..'
alias cccccc='cd ../../../../../..'

# search
alias f='find . | grep --color=auto -i'
alias g='grep --color=auto -nirEI'

